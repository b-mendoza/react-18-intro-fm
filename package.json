{
  "name": "react-18-vite-intro",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run lint && vite build",
    "dev": "vite",
    "fix": "pnpm run \"/^fix:.*/\"",
    "fix:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "fix:prettier": "prettier --ignore-unknown --cache --write .",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --ignore-unknown --cache --check .",
    "lint:types": "tsc --pretty",
    "preview": "vite preview"
  },
  "dependencies": {
    "nanoid": "5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.5.1",
    "@types/node": "20.7.0",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitejs/plugin-react-swc": "3.4.0",
    "eslint": "8.50.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "prettier": "3.0.3",
    "prettier-plugin-jsdoc": "1.0.2",
    "prettier-plugin-organize-imports": "3.2.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.1"
  }
}
